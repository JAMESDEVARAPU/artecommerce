{
  "name": "handmade-by-tejasree",
  "version": "1.0.0",
  "type": "module",
  "engines": {
    "node": "20.x"
  },
  "scripts": {
    "dev": "set NODE_ENV=development && tsx server/index.ts",
    "build": "vite build --outDir dist/public",
    "build:client": "vite build --outDir dist/public",
    "build:server": "tsc server/index.ts --outDir dist --target es2022 --module es2022 --moduleResolution node",
    "start": "node dist/index.js",
    "vercel-build": "npm run build:client"
  },
  "dependencies": {
    "@types/nodemailer": "^6.4.19",
    "bcrypt": "^5.1.1",
    "express": "^4.21.1",
    "express-session": "^1.18.1",
    "memorystore": "^1.6.7",
    "nodemailer": "^6.9.16"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/express": "^5.0.0",
    "@types/express-session": "^1.18.0",
    "@types/react": "^18.3.11",
    "@types/react-dom": "^18.3.1",
    "@vitejs/plugin-react": "^4.7.0",
    "autoprefixer": "^10.4.23",
    "dotenv": "^17.2.3",
    "postcss": "^8.5.6",
    "tailwindcss": "^3.4.0",
    "tsx": "^4.19.2",
    "typescript": "^5.6.3",
    "vite": "^5.4.20"
  }
}